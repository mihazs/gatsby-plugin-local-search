{
  "name": "gatsby-plugin-local-search-ml",
  "version": "0.0.1",
  "description": "Gatsby plugin for providing client-side search for data available in Gatsby's GraphQL layer",
  "scripts": {
    "format": "prettier --write 'src/**/*.js' 'README.md'",
    "build": "babel src --out-dir dist",
    "prepublish": "npm run build",
    "watch": "npm run build -- --watch",
    "release": "standard-version"
  },
  "homepage": "https://github.com/mihazs/gatsby-plugin-local-search",
  "repository": {
    "type": "git",
    "url": "https://github.com/mihazs/gatsby-plugin-local-search.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "lunr",
    "search"
  ],
  "author": "Mihael Zamin <mihael.zamine@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "husky": "^1.3.1",
    "prettier": "^1.16.4",
    "standard-version": "^5.0.2"
  },
  "dependencies": {
    "flexsearch": "^0.6.21",
    "gatsby-node-helpers": "^0.3.0",
    "lodash.lowerfirst": "^4.3.1",
    "lunr": "^2.3.6",
    "lunr-languages": "^1.1.0",
    "ramda": "^0.26.1"
  },
  "peerDependencies": {
    "gatsby": ">=2.2.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
