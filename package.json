{
  "name": "gatsby-plugin-local-search-ml",
  "version": "0.0.1",
  "description": "Gatsby plugin for providing client-side search for data available in Gatsby's GraphQL layer",
  "scripts": {
    "build": "babel src --out-dir . --ignore __tests__",
    "watch": "babel -w src --out-dir . --ignore __tests__",
    "prepare": "cross-env NODE_ENV=production npm run build"
  },
  "homepage": "https://github.com/mihazs/gatsby-plugin-local-search",
  "repository": {
    "type": "git",
    "url": "https://github.com/mihazs/gatsby-plugin-local-search.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "lunr",
    "search"
  ],
  "author": "Mihael Zamin <mihael.zamine@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "cross-env": "^5.0.5",
    "prettier": "^1.16.4",
    "standard-version": "^5.0.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "flexsearch": "^0.6.21",
    "gatsby-node-helpers": "^0.3.0",
    "lodash.lowerfirst": "^4.3.1",
    "lunr": "^2.3.6",
    "lunr-languages": "^1.1.0",
    "ramda": "^0.26.1"
  },
  "peerDependencies": {
    "gatsby": ">=2.2.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
